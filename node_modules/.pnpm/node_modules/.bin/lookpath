#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/lookpath@1.2.2/node_modules/lookpath/bin/node_modules:/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/lookpath@1.2.2/node_modules/lookpath/node_modules:/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/lookpath@1.2.2/node_modules:/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/lookpath@1.2.2/node_modules/lookpath/bin/node_modules:/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/lookpath@1.2.2/node_modules/lookpath/node_modules:/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/lookpath@1.2.2/node_modules:/home/rominlinux/work/romin-soft-zap-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lookpath/bin/lookpath.js" "$@"
else
  exec node  "$basedir/../lookpath/bin/lookpath.js" "$@"
fi
